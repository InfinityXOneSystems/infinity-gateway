{
  "name": "infinity-gateway",
  "version": "1.0.0",
  "description": "FAANG-grade Enterprise Gateway Hub - Consolidated MCP, AI, Memory, Orchestrator, Security, Monitoring, PubSub, GitHub, Google Workspace, and Admin services",
  "main": "dist/server/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "docker:build": "docker build -t infinity-gateway .",
    "docker:run": "docker run -p 8080:8080 infinity-gateway",
    "deploy:gcp": "gcloud run deploy infinity-gateway --source . --platform managed --region us-central1 --allow-unauthenticated"
  },
  "keywords": [
    "gateway",
    "mcp",
    "ai",
    "orchestrator",
    "enterprise",
    "microservices",
    "google-cloud",
    "github",
    "google-workspace"
  ],
  "author": "InfinityXone",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.17.1",
    "@google-cloud/pubsub": "^4.0.6",
    "@google-cloud/storage": "^7.7.0",
    "@googleapis/calendar": "^3.0.0",
    "@googleapis/docs": "^3.0.0",
    "@googleapis/drive": "^8.0.0",
    "@googleapis/gmail": "^8.0.0",
    "@googleapis/sheets": "^5.0.0",
    "@octokit/app": "^16.1.2",
    "@octokit/rest": "^20.0.2",
    "@octokit/webhooks": "^12.0.10",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "chalk": "^5.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "google-auth-library": "^9.6.3",
    "googleapis": "^169.0.0",
    "groq-sdk": "^0.3.3",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "openai": "^4.20.1",
    "prometheus-api-metrics": "^3.2.2",
    "redis": "^4.6.10",
    "socket.io": "^4.7.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
